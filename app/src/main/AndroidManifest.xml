<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uw.tacoma.mmuppa.layoutexamples" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".RelativeLayoutActivity"
            android:label="@string/relative_app_name" >
        </activity>
        <activity
            android:name=".LinearLayoutActivity"
            android:label="@string/linear_app_name" >
        </activity>
        <activity
            android:name=".WebViewActivity"
            android:label="@string/webview_app_name" >
        </activity>
        <activity
            android:name=".ImageViewActivity"
            android:label="@string/grid_app_name" >
        </activity>
        <activity
            android:name=".GridLayoutActivity"
            android:label="@string/grid_app_name" >
        </activity>
        <activity
            android:name=".TableLayoutActivity"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
